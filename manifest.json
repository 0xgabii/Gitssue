{
  "key": "",        
  "name": "Gitssue",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Manage your github issue",

  "browser_action": {
    "default_popup": "./popup/index.html"
  },

  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "content/inject/style.css",        
        "content/capture/lib/cropper.min.css"
      ],
      "js": [
        "content/inject/index.js",
        "content/message.js",        
        "content/auth.js",
        "content/ui.js",
        "content/capture/index.js",
        "content/capture/lib/cropper.min.js"        
      ]
    }
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "oauth2": {
    "client_id": "868497383059-aqol75cjtsi69jiralrj1827cp7l87nu.apps.googleusercontent.com",
    "scopes": []
  },

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  
  "permissions": [
    "<all_urls>",
    "activeTab",
    "storage",
    "identity",
    "http://*/*",
    "https://*/*"
  ]
}